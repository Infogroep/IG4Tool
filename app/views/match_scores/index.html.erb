<h1>Listing match_scores</h1>

<table>
	<tr>
		<th>Match</th>
		<th>User</th>
		<th>Score</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>

<% @match_scores.each do |match_score| %>
	<tr>
		<td><%= match_score.match_id %></td>
		<td><%= match_score.user_id %></td>
		<td><%= match_score.score %></td>
		<td><%= link_to t(:show), match_score %></td>
		<td><%= link_to t(:edit), edit_match_score_path(match_score) %></td>
		<td><%= link_to t(:destroy), match_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	</tr>
<% end %>
</table>

<br />

<%= link_to t(:new, :model => t(:match_score, :count => 1)), new_match_score_path %>
