<!DOCTYPE html>
<html>
	<head>
		<title><%= content_for?(:title) ? yield(:title) : "Untitled" %></title>
		<%= stylesheet_link_tag "application" %>
		<%= javascript_include_tag :defaults %>
		<%= csrf_meta_tag %>
		<%= yield(:head) %>
	</head>
	<body>
		<div id="container">
			<%= render :partial => 'layouts/header' %>
			<%= render :partial => 'layouts/content' %>
		</div>
	</body>
</html>
