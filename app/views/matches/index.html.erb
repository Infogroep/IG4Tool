<h1>Listing matches</h1>

<table>
	<tr>
		<th>Compo</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>

	<% @matches.each do |match| %>
		<tr>
			<td><%= match.compo_id %></td>
			<td><%= link_to t(:show), match %></td>
			<td><%= link_to t(:edit), edit_match_path(match) %></td>
			<td><%= link_to t(:destroy), match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<% end %>
</table>

<br/>

<%= link_to t(:new, :model => t("match.one")), new_match_path %>
