<%= simple_form_for(@team) do |f| %>
		<%= f.input :name %>
		<%= f.association :compo, label_method: :name %>
		<%= f.submit %>

	<%= f.association :users, collection: User.all, :label_method => :username, :input_html => {"data-placeholder" => 'Chose some Exercises', class: 'chosen'}  %>
<% end %>
