<h1>Listing clans</h1>

<table>
	<tr>
		<th>Tag</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>

	<% @clans.each do |clan| %>
		<tr>
			<td><%= clan.tag %></td>
			<td><%= link_to t(:show), clan %></td>
			<td><%= link_to t(:edit), edit_clan_path(clan) %></td>
			<td><%= link_to t(:destroy), clan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<% end %>
</table>

<br/>

<%= link_to t(:new, :model => t("clan.one")), new_clan_path %>
