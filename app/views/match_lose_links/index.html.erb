<h1>Listing match_lose_links</h1>

<table>
	<tr>
		<th>Match</th>
		<th>Next match</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>

<% @match_lose_links.each do |match_lose_link| %>
	<tr>
		<td><%= match_lose_link.match_id %></td>
		<td><%= match_lose_link.next_match_id %></td>
		<td><%= link_to 'Show', match_lose_link %></td>
		<td><%= link_to 'Edit', edit_match_lose_link_path(match_lose_link) %></td>
		<td><%= link_to 'Destroy', match_lose_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
	</tr>
<% end %>
</table>

<br />

